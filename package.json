    "@emotion/styled":"^11.10.6"
  },
  "devDependencies": {
    "jest":"^29.5.0",
    "@types/jest":"^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "@types/chrome": "^0.0.234",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "css-loader": "^6.7.3",
