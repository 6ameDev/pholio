{
  "manifest_version": 3,
  "name": "Pholio",
  "description": "Gather all your Investments data at one place (browser extension)",
  "version": "0.1",
  "devtools_page": "devtools.html",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "webNavigation"
  ]
}